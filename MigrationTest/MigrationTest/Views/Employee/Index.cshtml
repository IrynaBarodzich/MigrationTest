@model IEnumerable<MigrationTest.Models.Employee>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Employees";
}

<table> 
    <tr> 

        <th>Id Employee</th> 
         <th>Name</th> 
        <th></th> 

                <th></th> 
    </tr> 


@foreach (var item in Model) { 
    <tr> 
         <td> 
            @Html.DisplayFor(modelItem => item.EmployeeID) 
        </td>
        <td> 
            @Html.DisplayFor(modelItem => item.EmployeeName) 
        </td> 
 
        <td> 
            @Html.ActionLink("Chiefs", "ChiefList", "Employee", new { id=item.EmployeeID }, null)
        </td> 
   
    </tr> 
} 
    

    
 
</table>
